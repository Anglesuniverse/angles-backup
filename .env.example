# Angles AI Universeâ„¢ Environment Variables
# Copy this file to .env and fill in your actual values

# Supabase Configuration (Required)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# Notion Configuration (Required for sync)
NOTION_TOKEN=secret_your-notion-integration-token
NOTION_API_KEY=secret_your-notion-integration-token
NOTION_DATABASE_ID=your-notion-database-id

# GitHub Configuration (Required for backups)
GITHUB_TOKEN=ghp_your-github-personal-access-token
REPO_URL=https://github.com/username/repo-name.git
GIT_USERNAME=your-git-username
GIT_EMAIL=your-git-email@example.com

# OpenAI Configuration (Optional)
OPENAI_API_KEY=sk-your-openai-api-key

# Processing Configuration
PROCESSING_INTERVAL=30
BATCH_SIZE=10
MAX_RETRIES=3
RETRY_DELAY=5

# Data Paths
INPUT_DATA_PATH=./data/input
PROCESSED_DATA_PATH=./data/processed
ERROR_DATA_PATH=./data/errors

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=gpt_processor.log

# Schema Configuration
TABLE_SCHEMAS_PATH=./config/table_schemas.json

# Memory System Configuration
MEMORY_RETENTION_DAYS=30
AUTO_CLASSIFY=true

# Performance Settings
MAX_CONCURRENT_OPERATIONS=5
CONNECTION_TIMEOUT=30

# Scheduler Settings
SCHEDULER_LOG_LEVEL=INFO
HEALTH_CHECK_INTERVAL=3600
BACKUP_INTERVAL=21600
